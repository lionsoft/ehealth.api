{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Medication Request request identifier"
      },
      "status": {
        "type": "string",
        "description": "Medication Request status. Find available **statuses** on State Charts wiki page"
      },
      "request_number": {
        "type": "string",
        "description": "Public medication request human readable number"
      },
      "created_at": {
        "type": "string",
        "description": "Medication request creation date, which is determined by the external system. Format *DATE '2017-09-07'*"
      },
      "started_at": {
        "type": "string",
        "description": "Start date of a treatment period, which is determined by the external system. Greater or equal to **created_at**. Format *DATE '2017-09-07'*"
      },
      "ended_at": {
        "type": "string",
        "description": "End date of a treatment period, which is determined by the external system. Greater or equal to **started_at**. Format *DATE '2017-10-07'*"
      },
      "dispense_valid_from": {
        "type": "string",
        "description": "Start date of dispense period, which is determined by the external system. Format *DATE '2017-09-07'*"
      },
      "dispense_valid_to": {
        "type": "string",
        "description": "End date of dispense period, which is determined by the external system. Greater or equal to **dispense_valid_from**. Format *DATE '2017-10-07'*"
      },
      "legal_entity": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "short_name": {
            "type": "string"
          },
          "public_name": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "edrpou": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "enum": [
              "ACTIVE",
              "CLOSED"
            ]
          }
        },
        "required": [
          "id",
          "name",
          "short_name",
          "public_name",
          "type",
          "edrpou",
          "status"
        ]
      },
      "division": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "addresses": {
            "type": "array"
          },
          "phones": {
            "type": "array"
          },
          "email": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "external_id": {
            "type": "string"
          },
          "location": {
            "type": "object",
            "properties": {
              "latitude": {
                "type": "number"
              },
              "longitude": {
                "type": "number"
              }
            },
            "required": [
              "latitude",
              "longitude"
            ]
          }
        },
        "required": [
          "id",
          "name",
          "addresses",
          "phones",
          "email",
          "type"
        ]
      },
      "employee": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "position": {
            "type": "string",
            "description": "`Dictionary POSITION`"
          },
          "party": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "first_name": {
                "type": "string"
              },
              "last_name": {
                "type": "string"
              },
              "second_name": {
                "type": "string"
              },
              "email": {
                "type": "string"
              },
              "phones": {
                "type": "array"
              },
              "tax_id": {
                "not": {}
              },
              "documents": {
                "not": {}
              }
            },
            "required": [
              "id",
              "first_name",
              "last_name"
            ]
          }
        },
        "required": [
          "id",
          "position"
        ]
      },
      "person": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "first_name": {
            "type": "string"
          },
          "last_name": {
            "type": "string"
          },
          "second_name": {
            "type": "string"
          },
          "age": {
            "type": "integer",
            "description": "Full years between birth date and medication request date"
          }
        },
        "required": [
          "id",
          "first_name",
          "last_name",
          "age"
        ]
      },
      "medication_info": {
        "type": "object",
        "properties": {
          "medication_id": {
            "type": "string",
            "description": "`Identifier medication with type **INNM_DOSAGE**`"
          },
          "medication_name": {
            "type": "string",
            "description": "`Name of medication with type **INNM_DOSAGE**`"
          },
          "form": {
            "type": "string",
            "description": "`Dictionary MEDICATION_FORM`"
          },
          "dosage": {
            "type": "object",
            "properties": {
              "numerator_unit": {
                "type": "string",
                "description": "`Dictionary MEDICATION_UNIT`"
              },
              "numerator_value": {
                "type": "number"
              },
              "denumerator_unit": {
                "type": "string",
                "description": "`Dictionary MEDICATION_UNIT`"
              },
              "denumerator_value": {
                "type": "number"
              }
            },
            "required": [
              "numerator_unit",
              "numerator_value",
              "denumerator_unit",
              "denumerator_value"
            ],
            "description": "Dosage of innm_dosage object"
          },
          "medication_qty": {
            "type": "number",
            "description": "Medication quantity isuued by the doctor"
          }
        },
        "required": [
          "medication_id",
          "medication_name",
          "form",
          "dosage",
          "medication_qty"
        ]
      },
      "medical_program": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "`Medical program Identifier`"
          },
          "name": {
            "type": "string",
            "description": "`Medical program name`"
          }
        },
        "required": [
          "id",
          "name"
        ]
      }
    },
    "required": [
      "id",
      "status",
      "request_number",
      "created_at",
      "started_at",
      "ended_at",
      "dispense_valid_from",
      "dispense_valid_to",
      "legal_entity",
      "division",
      "employee",
      "person",
      "medication_info",
      "medical_program"
    ],
    "additionalProperties": false
  }
}
